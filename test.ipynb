    "def all_two_squares(n):\n",
    "    return [(abs(d[0]),abs(d[1])) for d in divisors(GaussianIntegers()(n)) if norm(d)==n]\n",
    "\n",
    "\n",
    "n=73\n",
    "\n",
    "all=all_two_squares(n)\n",
    "\n",
    "count= len(all)/2\n",
    "\n",
    "print(f'There is {count} representations of {n}.')\n",
    " \n",
    "all"
   ]
  }
